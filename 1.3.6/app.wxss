/**app.wxss**/
/*隐藏滚动条  */
::-webkit-scrollbar{width: 0;height: 0;color: transparent;}
.clearfix {
    zoom: 1;
}

.clearfix::after {
    content: "";
    display: block;
    height: 0;
    clear: both;
}

.rp-flex {
    display: flex;
}

.rp-flex-column {
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 100%;
}

.rp-flex-item {
    flex: 1;
    align-items: center;
}

.justify-center {
    justify-content: center;
}

.align-center {
    align-items: center;
}

.mar-l0 {
    margin-left: 0!important;
}

.bb-none {
    border-bottom: none!important;
}

.mar-b0 {
    margin-bottom: 0!important;
}

.overflow-ellipsis {
    display: -webkit-box!important;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.overflow-ellipsis3 {
    display: -webkit-box!important;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}

.v-middle {
    display: inline-block;
    vertical-align: middle;
}

.order-group-btn>.rp-btn {
    margin-left: 20rpx;
}

.rp-btn {
    display: inline-block;
    padding: 0 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    border-radius: 4rpx;
    border: 1rpx solid #2d2d2d;
    color: #2d2d2d;
}

.rp-btn.canel {
    border: 1rpx solid #2d2d2d;
    color: #2d2d2d;
}

.rp-btn.gopay,
.rp-btn.red {
    border: 1rpx solid #fe6305;
    color: #fe6305;
}
.rp-btn.blue{
   border: 1rpx solid #31a6f6;
    color: #31a6f6;
}
.rp-btn.orange{
   border: 1rpx solid #ff8855;
    color: #ff8855;
}
.rp-btn.yellow{
   border: 1rpx solid #f4c744;
    color: #f4c744;
}
.rp-btn.green{
   border: 1rpx solid #09ba08;
    color: #09ba08;
}
.rp-btn.rred{
   border: 1rpx solid #ff4445;
    color: #ff4445;
}

.rp-btn.red.time{
    padding: 0;
    width: 324rpx;
    text-align: center;
}


