/*打烊弹出框*/

.rp-dialog {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: transparent;
    z-index: 100;
}

.rp-dialog .mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .5);
}

.rp-dialog .body {
    position: absolute;
    left: 50%;
    top: 50%;
    background: #fff;
    width: 518rpx;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    text-align: center;
    z-index: 1000;
    border-radius: 12rpx;
    overflow: hidden;
}

.rp-dialog .content {
    padding: 80rpx 20rpx 70rpx 20rpx;
    color: #000000;
}

.rp-dialog .title {
    font-size: 30rpx;
    margin-bottom: 20rpx;
}

.rp-dialog .txt {
    font-size: 28rpx;
}

.rp-dialog .footer {
    height: 94rpx;
    line-height: 94rpx;
    font-size: 30rpx;
    color: #31a6f6;
    border: 1rpx solid #f1f1f1;
}

.v-block {
    display: block;
}

.v-hide {
    display: none!important;
}

.message {
    position: fixed;
    top: 80%;
    left: 50%;
    width: 400rpx;
    height: 80rpx;
    margin-left: -200rpx;
    background-color: rgba(0, 0, 0, .75);
    border-radius: 20px;
    text-align: center;
    line-height: 80rpx;
    color: #fff;
    opacity: 0;
    font-size: 28rpx;
}

.fade-in {
    animation: fadeIn .3s forwards;
}

@keyframes fadeIn {
    from {
        opacity: 0;
        -webkit-transform: translate3d(0, 100%, 0);
        transform: translate3d(0, 100%, 0);
    }
    to {
        opacity: 1;
        -webkit-transform: none;
        transform: none;
    }
}



