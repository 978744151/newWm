page {
    position: relative;
    width: 100%;
    height: 100%;
}

.power-wapper {
    padding-top: 314rpx;
    text-align: center;
    font-size: 28rpx;
}

.power-wapper .icon-on-power {
    display: inline-block;
    font-size: 140rpx;
}

.power-wapper .tip {
    margin: 22rpx 0 40rpx;
    padding: 0 100rpx;
    color: #999;
    line-height: 150%;
}

.power-wapper .btn {
    display: inline-block;
    height: 56rpx;
    line-height: 56rpx;
    border-radius: 4rpx;
    background: #31a6f6;
    color: #fff;
    padding: 0 34rpx;
}
