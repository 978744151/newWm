.clearfix::after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.panel {
    width: 100%;
    background-color: #fff;
    margin-bottom: 20rpx;
    overflow: hidden;
    color: #999;
}

.panel-title {
    font-size: 32rpx;
    color: #333;
}

.cell {
    margin: 0 0 0 30rpx;
    color: #666;
    font-size: 30rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #ebeded;
    position: relative;
}

.cell-l-box {
    display: inline-block;
    vertical-align: top;
}

.cell-r-box {
    display: inline-block;
    position: relative;
    width: 500rpx;
    padding-top: 14rpx;
}

.cell-r-box text {
    display: block;
    line-height: 50rpx;
}

.line {
    padding: 0 30rpx;
    color: #666;
    font-size: 28rpx;
    line-height: 80rpx;
    border-bottom: 2rpx solid #ebeded;
    position: relative;
}

.icon-call {
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 30rpx;
    width: 40rpx;
    height: 30rpx;
    display: inline-block;
    position: relative;
    top: 5rpx;
}

.order-status {
    text-align: center;
    padding: 20rpx 0;
}

.order-status .logo {
    width: 150rpx;
    height: 150rpx;
    border: 1rpx solid #eeeeee;
    display: inline-block;
    margin-bottom: 30rpx;
}

.order-status .logo image {
    width: 100%;
    height: 100%;
}

.order-status .status {
    line-height: 80rpx;
    color: #333333;
}

.order-status .des {
    font-size: 24rpx;
    line-height: 38rpx;
    color: #999999;
}

.order-status .action {}

.order-status .action .btn,
.cart-info .call-shop .content {
    border: 2rpx solid #cccccc;
    border-radius: 8rpx;
    line-height: 90rpx;
    font-size: 26rpx;
    padding: 14rpx 24rpx;
    color: #666666;
    margin-right: 10rpx;
}

.order-status .action .btn.red {
    background-color: #31a6f6;
    color: #fff;
    border: 2rpx solid #31a6f6;
}

.cart-info .shop-info .logo {
  border: 1px solid #ddd;
    width: 50rpx;
    height: 50rpx;
    position: relative;
    top: 15rpx;
    margin-right: 10rpx;
}

.cart-info .call-shop {
    text-align: center;
}

.cart-info .call-shop text {
    padding: 14rpx 28rpx;
}

.cart-items {
    height: 100%;
    border: 0;
}

.cart-items .cart-items-list {
     border-bottom: 1rpx solid #ebeded; 
}
.cart-items .cart-item .name {
    width: 75%;
    display: inline-block;
    position: relative;
}
.cart-items .cart-item .name.last{margin-bottom: 15rpx;}
.cart-items .cart-item .name .skuClass{
  position:absolute;
  left: 0;
  top: 35rpx;
  color: #919191;
  font-size: 24rpx;
}
.cart-items .cart-item .num {
    width: 10%;
    display: inline-block;
    text-align: center;
}

.cart-items .cart-item .price {
    width: 15%;
    display: inline-block;
    text-align: right;
}

.cart-items .cart-item-total {
    /* text-align: right; */
}

.order-group-btn.order-detail {
    padding: 30rpx 0 10rpx;
}


