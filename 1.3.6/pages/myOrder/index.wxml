 <import src="../DEMOTAB/DEMOTAB.wxml"/>
 <template is="tabbar" data="{{tabbar}}"/>
 <view class="content">
     <!--顶部切换-->
     <view class="listnav">
         <view class="navitem {{cpage == 0 ? 'active' : ''}}" bindtap="changePage" data-page='0'>
             <text>排队号单</text>
         </view>
         <view class="navitem {{cpage == 1 ? 'active' : ''}}" bindtap="changePage" data-page='1'>
             <text>点菜订单</text>
         </view>
     </view>
     <!--顶部切换end-->
     <view style="height:90rpx;"></view>
     <!--暂无订单-->
     <view class="rp-undata-default">
         <view class="undata-icon"></view>
         <view class="undata-txt">没有相关订单</view>
     </view>
 </view>