    text-align: center;
    padding: 25rpx 0;
    width: 100%;
}

.rp-foot .img {
    width: 100%;
    height: 84rpx;
    background: url('https://pic.repaiapp.com/pic/05/25/31/052531731aeab17ee0b85a11c74a7da348631ee0.png') center no-repeat;
    background-size: auto 84rpx;
}

/*商品属性sku  */

.sku {
    position: absolute;
    left: 50%;
    top: 40%;
    width: 85%;
    transform: translate(-50%, -50%);
    background: #fff;
    z-index: 52;
    border-radius: 4px;
}

.sku .skuTit {
    line-height: 104rpx;
    text-align: center;
    font-size: 34rpx;
    color: #000;
     width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis; 
}

.sku .skuClose {
    position: absolute;
    width: 68rpx;
    height: 68rpx;
    background: url('https://pic.repaiapp.com/pic/2d/28/a5/2d28a5a3ae250e1fc9f8049f3023186330f18ab9.png')no-repeat;

left:50%;
transform:translateX(-50%);
bottom:-120rpx;
background-size:68rpx 68rpx;
z-index:102;

}

.sku-scroll {
    height: 550rpx;
}

.skuList {
    padding: 0 30rpx;
}

.skuList .skuTitle {
    font-size: 28rpx;
    color: #a1a1a1;
    margin-bottom: 17rpx;
}

.skuCon text {
    display: inline-block;
    font-size: 26rpx;
    color: #999;
    padding: 17rpx 44rpx;
    background: #f5f5f5;
    border-radius: 3px;
    margin-right: 17rpx;
    margin-bottom: 27rpx;
    font-weight: 600;
}

