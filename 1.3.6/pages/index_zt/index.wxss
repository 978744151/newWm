.fade-transition {
    -webkit-transition: all 2s;
    transition: all 2s;
    opacity: 1;
    background: rgba(0, 0, 0, 0.5);
}

@keyframes fadeIn {
    from {
        opacity: 0;
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }

    to {
        opacity: 1;
        -webkit-transform: translate3d(0, -100%, 0);
        transform: translate3d(0, -100%, 0);
    }
}

.fold-transition {
    -webkit-transition: all 2s;
    transition: all 2s;
    -webkit-transform: translate3d(0, -100%, 0);
