page {
    background: #f6f6f6;
    height: 100%;
}

.clear {
    clear: both
}

.swiper {
    width: 100%;
    height: 246rpx;
}

.banner-img {
    width: 100%;
    height: 247rpx;
}

.banner-img image {
    width: 100%;
    height: 247rpx;
}

.Integral {
    width: 100%;
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
    background: #fff;
    line-height: 88rpx;
    height: 88rpx;
    position: relative;
    left: 0px;
    top: 0px;
    border-bottom: 1rpx #f0f0f0 solid;
}

.Integral .item {
    text-align: center;
    height: 62rpx;
    line-height: 60rpx;
    flex: 1;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #010101;
    position: relative;
}

.Integral .item text {
    vertical-align: middle;
}

.Integral .item:first-child {
    border-right: 1px #f0f0f0 solid;
}

.Integral text.clr-y {
    color: #ff7b11;
    font-size: 32rpx;
}

.exchange {
    margin-top: 10px;
    position: relative;
    background: #fff;
    padding: 15px 10px;
    font-size: 14px;
    color: #393939;
    border-top: 1px #f0f0f0 solid;
}
.exchangeList {
    position: relative;
    display: flex;
    flex: 0 0 50%;
    flex-flow: column nowrap;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    padding: 10px;
    border-right: 1px #f0f0f0 solid;
    border-bottom: 1px #f0f0f0 solid;
    background-color: #fff;
}

.exchangeList image {
    width: 100%;
    height: 220rpx;
}

.exchangeList:nth-child(2n) {
    border-left: none
}

.list-tit {
    padding: 25rpx 10rpx;
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
    width: 100%;
    box-sizing: border-box;
}

.list-tit text {
    padding: 5rpx 10rpx;
    border-radius: 4rpx;
    font-size: 23rpx;
    color: #fff;
    margin-right: 15rpx;
}

.Listname {
    font-size: 28rpx;
    font-weight: normal;
    background: #fff;
    flex: 1;
    box-sizing: border-box;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.jf {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;
    text-align: left;
    font-size: 14px;
    line-height: 24px;
    font-weight: normal;
    box-sizing: border-box;
    background: #fff;
    padding: 0 10rpx;
}

.jf .right-span {
    color: #ff7b11;
    font-size: 28rpx;
}

.jf-money {
    padding: 5rpx 10rpx;
    background: #4099d9;
    margin-left: 15rpx;
    color: #fff;
    font-size: 24rpx;
    border-radius: 4rpx;
}

.unmore {
    padding: 20rpx 0;
    font-size: 26rpx;
    color: #ccc;
    text-align: center;
}

.undata-wapper {
    margin-top: 22rpx;
    flex: 1;
}

.undata-wapper .icont-undata {
    margin-top: 120rpx;
    height: 164rpx;
    background: url('https://pic.repaiapp.com/pic/c4/91/09/c4910911e75901fd7c2181dbd292030dbf12ca0c.png') center no-repeat;
    background-size: auto 100%;
}

.undata-wapper .undata-text {
    font-size: 26rpx;
    color: #a2a2a2;
    margin-top: 22rpx;
    text-align: center;
}

.icont-integral {
    display: inline-block;
    width: 34rpx;
    height: 34rpx;
    margin-right: 20rpx;
    background: url('https://pic.repaiapp.com/pic/5a/3b/75/5a3b75dcdf015d37e3420843816afd9d9501831b.png') center no-repeat;
    background-size: cover;
}

.icont-exchange {
    display: inline-block;
    width: 34rpx;
    height: 34rpx;
    margin-right: 20rpx;
    background: url('https://pic.repaiapp.com/pic/68/e1/03/68e1038294f29bdf3971d069842714a5082551ba.png') center no-repeat;
    background-size: cover;
}