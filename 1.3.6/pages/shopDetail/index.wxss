/*index.wxss 商家详情*/

page {
    background: #f6f6f6;
}

.rp-panel {
    background: #fff;
    margin-bottom: 16rpx;
}


/*header 头部**/

.base-header-wapper {
    background: #fff;
    padding: 30rpx 22rpx;
    color: #727272;
    font-size: 23rpx;
    margin-bottom: 16rpx;
}

.base-header-wapper .logo {
    width: 158rpx;
    height: 158rpx;
    margin-right: 36rpx;
    border-radius: 50%;
    background: #fff;
    overflow: hidden;
    border: 1rpx solid #eeeeee;
}

.base-header-wapper .logo>image {
    width: 100%;
    height: 100%;
}

.base-header-wapper .title {
    display: block;
    font-size: 32rpx;
    color: #2c2c2c;
    margin: 26rpx 0 15rpx;
}

.base-header-wapper .send-tag {
    display: inline-block;
    height: 34rpx;
    line-height: 34rpx;
    padding: 0 8rpx;
    color: #fff;
    background: #31a6f6;
    margin-right: 20rpx;
    border-radius: 4rpx;
}

.base-header-wapper .other-tip {
    margin-top: 16rpx;
}

.base-header-wapper .other-tip>text {
    margin: 0 12rpx;
}

.base-header-wapper .notice-wapper {
    background: #f6f6f6;
    border-radius: 4rpx;
    font-size: 23rpx;
    color: #727272;
    margin-top: 30rpx;
    padding: 0 22rpx;
}

.base-header-wapper .notice-wapper .item {
    padding: 25rpx 0;
    line-height: 36rpx;
    border-top: 1rpx solid #eeeeee;
}

.border-t-none {
    border-top: none!important;
}


/*商家信息*/

.shop-title {
    height: 84rpx;
    line-height: 84rpx;
    font-size: 30rpx;
    color: #2c2c2c;
    padding: 0 22rpx;
    border-bottom: 1rpx solid #e5e5e5;
}

.rp-list-group {
    padding: 28rpx 22rpx;
    border-bottom: 1rpx solid #e5e5e5;
    color: #7d7d7d;
    font-size: 28rpx;
}

.rp-list-group .iconfont {
    display: inline-block;
    font-size: 28rpx;
    margin-right: 14rpx;
}

.rp-list-group .iconfont.icon-right-more {
    margin-right: 0!important;
}

.rp-g-3 {
    float: left;
    width: 33.33333333%;
    margin-bottom: 22rpx;
    text-align: center;
}

.image-mode {
    display: inline-block;
    width: 188rpx;
    height: 188rpx;
    background: #f6f6f6 url(data:image/png;base64,) center no-repeat;
    background-size: cover;
}

.shop-images-blank {
    padding: 0 11rpx;
    margin-top: 22rpx;
}

.padd-lr11 {
    padding: 0 11rpx;
}

/*活动区域  */
.activity-box{font-size: 26rpx;background: #fff;margin-bottom: 16rpx;padding: 0 22rpx 35rpx;}
.activity-box .activity-tit{font-size: 30rpx;padding-top: 27rpx;display: block;}
.activity{margin-top: 25rpx;}
.activity image{width: 24rpx;height: 24rpx;margin-right: 12rpx;}
.activity text{display: inline-block;}
/*小店技术支持  */
.footerimg{
    position: relative;
    width: 100%;
    height: 78rpx;
    text-align: center;
    margin-top: 40rpx;
}
.footerimg image{
    width:  260rpx;
    height:  78rpx;
}
.bg{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 100;
}
.bgimg{
    position: fixed;
    bottom: 0;
    background-color: #fff;
    width: 100%;
    padding:50rpx 0rpx;
    text-align: center;
    font-size: 28rpx;
    color: #7c7c7c;
}
.erweima{
    width: 205rpx;
    height: 205rpx;
    margin-bottom: 40rpx;
}
.bgimg view{
    margin-bottom: 20rpx
}
.xiaodian{
    width: 84rpx;
    height: 44rpx;
    margin-top: 20rpx;
}
.changan{
    position: relative;
    display: flex;
    top: 35%;
}
.leftfooter{
    position: relative;
    width: 50%;
    text-align: center;
    color: #fff;
    font-size: 28rpx;
}
.changanleft{
    width: 87rpx;
    height: 87rpx;
    margin-bottom: 20rpx;
}
.changanright{
    width: 98rpx;
    height: 87rpx;
    margin-bottom: 20rpx;
}