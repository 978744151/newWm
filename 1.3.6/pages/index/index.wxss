  /*轮播  */
  
  .swiper, .swiper .slideImage {
    width: 100%;
    height: 240rpx;
  }
  .swiper{border-bottom: 1px solid #f0f0f0;}
  .swiper .slideImage image, navigator {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
  }
  
  /*推荐  */
  
  .recommend, .appraise {
    background: #fff;
    margin-bottom: 10rpx;
  }
  
  .recommend image {
    width: 162rpx;
    height: 162rpx;
  }
  
  .indexTitle {
    padding-left: 20rpx;
    border-bottom: 1px solid #f0f0f0;
    line-height: 90rpx;
    font-size: 30rpx;
  }
  
  .indexTitle view {
    padding-left: 46rpx;
    background: url('https://pic.repaiapp.com/pic/85/bc/5a/85bc5aca749888d7dc844eef759d093cac42d525.png')no-repeat left center;
    background-size: 31rpx;
  }
  
  .recommend .scrollLIst {
    padding: 10rpx 0 6rpx 20rpx;
    position: relative;
  }
  
  .recommend .scrollLIst .goodsHandle {
    white-space: nowrap;
  }
  
  .recommend .scrollLIst .goodsBox {
    display: inline-block;
    width: 162rpx;
    box-sizing: border-box;
    overflow: hidden;
    margin-right: 20rpx;
  }
  
  .scrollLIst .goodsBox view {
    padding-left: 10rpx;
    font-size: 28rpx;
  }
  
  .scrollLIst .goodsBox .name {
    padding-top: 8rpx;
    line-height: 42rpx;
  }
  
  .scrollLIst .goodsBox .price {
    color: #f14949;
    line-height: 40rpx;
  }
  
  .recommend .hintIcon {
    position: absolute;
    right: 0;
    top: 60rpx;
    width: 39rpx;
    height: 78rpx;
    background: url('https://pic.repaiapp.com/pic/f4/c7/6c/f4c76cabfb5a26e1520d3c626a9cff8fe65b7ce6.png')no-repeat;
    background-size: 39rpx 78rpx;
    z-index: 2;
  }
  
  .noReco {
    text-align: center;
    padding: 80rpx 0 48rpx;
  }
  
  .noReco image {
    width: 84rpx;
    height: 71rpx;
  }
  
  .noReco view {
    line-height: 76rpx;
    color: #a6a5a5;
  }
  
  /*用户评价  */
  
  .appraise {
    padding-bottom: 44rpx;
    margin-bottom: 0;
  }
  
  .appraise .indexTitle view {
    background-image: url('https://pic.repaiapp.com/pic/ca/c1/d3/cac1d3dd8f89c8edaa452373f415cd01f11a32a0.png');
  }
  
  .gather .goodRep {
    text-align: center;
    font-size: 30rpx;
    line-height: 66rpx;
  }
  
  .gather .goodRep text {
    color: #f14949;
  }
  
  .gather .appraiseState {
    padding: 0rpx 0 10rpx;
    justify-content: center;
    align-items: center;
    display: flex;
  }
  
  .gather .appraiseState .stateBox {
    width: 120rpx;
    text-align: center;
  }
  
  .gather .appraiseState .stateBox image {
    width: 50rpx;
    height: 50rpx;
  }
  
  .gather .appraiseState .stateBox view {
    font-size: 26rpx;
    line-height: 48rpx;
  }
  
  .appraiseConBox {
    padding-bottom: 24rpx;
  }
  
  .appraiseCon {
    display: flex;
    flex-direction: row;
    padding: 20rpx 20rpx 24rpx;
    border-top: 1px solid #f0f0f0;
    font-size: 28rpx;
  }
  
  .appraiseCon .userLogo {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    border: 1px solid #f0f0f0;
    box-sizing: border-box;
    overflow: hidden;
    margin-right: 16rpx;
    background: url('https://pic.repaiapp.com/static/png/20171220/10/1513738640239869949.png') no-repeat center;
    background-size: cover;
  }
  
  .appraiseCon .userLogo image {
    vertical-align: top;
    width: 100%;
    height: 100%;
  }
  
  .appraiseCon .detail {
    flex: 1;
    overflow: hidden;
  }
  
  .appraiseCon .detail .name {
    display: flex;
    justify-content: space-between;
    line-height: 1;
  }
  
  .appraiseCon .detail .name text {
    font-size: 24rpx;
    color: #9f9f9f;
    line-height: 30rpx;
  }
  
  .appraiseCon .detail .state {
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 16rpx 0 16rpx;
    line-height: 1;
  }
  
  .appraiseCon .detail .state image {
    vertical-align: top;
    width: 34rpx;
    height: 34rpx;
    margin-right: 10rpx;
  }
  
  .appraiseCon .detail .content {
    font-size: 28rpx;
    line-height: normal;
    word-break: break-all;
  }
  
  .imgViewBox image {
    vertical-align: top;
    width: 170rpx;
    height: 170rpx;
    margin-top: 16rpx;
    margin-right: 30rpx;
  }
  
  .gather .appraiseMore {
    width: 215rpx;
    height: 60rpx;
    border: 1px solid #f14949;
    color: #f14949;
    text-align: center;
    line-height: 60rpx;
    box-sizing: border-box;
    border-radius: 30rpx;
    margin: 0 auto;
    font-size: 28rpx;
  }
  
