.imageGroup {
    width: 100%;
    height: 240rpx;
    position: relative;
    box-sizing: border-box;
    border-bottom: 1rpx solid #f0f0f0;
    background: url('https://pic.repaiapp.com/static/png/20171220/10/1513738640239869949.png') no-repeat center;
    background-size: cover;
  }
  
  .imageGroup image {
    width: 100%;
    height: 100%;
  }
  
  .imageText {
    position: absolute;
    width: 100rpx;
    text-align: center;
    right: 0;
    bottom: 14rpx;
  }
  
  .imageText view {
    margin: 0 auto;
    width: 31rpx;
    height: 26rpx;
    background: url('https://pic.repaiapp.com/pic/a3/45/96/a34596385e60c3d6bc7c778962e019fafd00456e.png')no-repeat;
    background-size: 31rpx 26rpx;
  }
  
  .imageText text {
    font-size: 18rpx;
    color: #fefefe;
    line-height: 33rpx;
  }
  
  .shop {
    background: #fff;
    margin-bottom: 10rpx;
  }
  
  .shopInfo {
    display: flex;
    flex-direction: row;
    padding: 20rpx;
  }
  
  .shopInfo .shopLogo {
    width: 162rpx;
    height: 162rpx;
    border: 1px solid #f0f0f0;
    overflow: hidden;
    margin-right: 22rpx;
  }
  
  .shopInfo .shopLogo image {
    width: 100%;
    height: 100%;
  }
  
  .shopInfo .shopDetail {
    flex: 1;
  }
  
  .shopDetail .name {
    line-height: 56rpx;
    font-size: 32rpx;
  }
  
  .shopDetail .evaluate {
    line-height: 49rpx;
    font-size: 26rpx;
    color: #494949;
    display: flex;
    flex-direction: row;
    align-items: center;
  }
  
  .shopDetail .evaluate .star {
    display: flex;
    flex-direction: row;
  }
  
  .shopDetail .evaluate .star text {
    display: block;
    width: 23rpx;
    height: 22rpx;
    background: url('https://pic.repaiapp.com/static/png/20171205/14/1512453677251398102.png')no-repeat;
    background-size: 23rpx 22rpx;
    margin-right: 3rpx;
  }

    
  .shopDetail .timeSection {
    font-size: 26rpx;
    color: #494949;
    line-height: 52rpx;
    /* background: url('https://pic.repaiapp.com/pic/8d/8b/bf/8d8bbf4ce46247e0e4351302aaf8ae48c29a17eb.png') no-repeat left center; */
    background-size: 34rpx;
  }
  
  .icont-noticemohu {
   background-image:url('https://pic.repaiapp.com/pic/f7/a6/e0/f7a6e0e7e56c72bbe321e368b17b42e24bbc7d5b.png');
    
    }
    .icont-notice {
    display:inline-block;
    width:50rpx;
    height:28rpx;
    background-image:url('https://pic.repaiapp.com/pic/37/3c/8f/373c8f957c9ea9c4b503ea02fb0fc4e619df8a6b.png');
    background-position:center;
    background-repeat:no-repeat;
    background-size:cover;
    margin-right:10rpx;
    vertical-align:middle;
    
    }
    
  
  .shop-address{
    padding-left:20rpx;
    display: flex;
    align-items: center;
    color:#333;
  }
  
  .shop-address .icont-map{
    width: 30rpx;
    height: 38rpx;
    background: url('https://pic.repaiapp.com/static/png/20171221/13/1513833412889295252.png') no-repeat center;
    background-size: cover;
    margin-right: 15rpx;
  }
  
  
  .shop-address .address{
    flex: 1;
    font-size: 28rpx;
    padding: 16rpx 0;
  }
  
  .shop-address .btn-group{
    display: inline-flex;
    height: 100%;
   
  }
  
  .shop-address .item-gps,.shop-address .item-tel{
    position: relative;
    display: block;
    width:120rpx ;
    text-align: center;
    padding: 20rpx 0;
  }
  
  .shop-address .item-gps::after{
    content: '';
    position: absolute;
    right: 0;
    top: 50%;
    height: 50rpx;
    margin-top: -25rpx;
    border-left: 1px solid #ddd;
  }
  
  /* .shop-address .item-tel{
    padding-left: 20rpx;
  
  } */
  
  .shop-address .icont-gps{
    display: inline-block;
    width: 32rpx;
    height: 32rpx;
    background: url('https://pic.repaiapp.com/pic/28/3a/38/283a38913bf8686fced11fefda3e9b13a3b454d8.png') no-repeat center;
    background-size: cover;
    margin-bottom: 10rpx;
  }
  
  .shop-address .icont-tel{
    display: inline-block;
    width: 29rpx;
    height: 32rpx;
    background: url('https://pic.repaiapp.com/pic/bf/fc/0f/bffc0f57b54b5c838a40f43916579bf38f724de2.png') no-repeat center;
    background-size: cover;
    margin-bottom: 10rpx;
  }
  
  .shop-address .btn-group .txt{
    line-height: 1;
    font-size: 26rpx;
  }
  
  
  
  /*优惠券  */
  
  .coupons {
    padding: 24rpx 0 24rpx;
    border-top: 1px solid #f0f0f0;
  }
  
  .coupons .couponOne, .coupons .couponMore {
    height: 156rpx;
  }
  
  /*一个  */
  
  .coupons .couponOne {
    color: #c2a46b;
    width: 710rpx;
    height: 162rpx;
    margin: 0 auto;
    background: url('https://pic.repaiapp.com/static/png/20171204/21/1512394937748279997.png')no-repeat;
    background-size: 710rpx 162rpx;
    display: flex;
    flex-direction: row;
  }
  
  .coupons .couponOne .left {
    width: 479rpx;
    box-sizing: border-box;
    padding: 17rpx 0 17rpx 19rpx;
    display: flex;
    flex-direction: row;
  }
  
  .couponOne .left .money {
    width: 128rpx;
    height: 128rpx;
    margin-right: 25rpx;
  }
  
  .couponOne .left .money .comCoupon image {
    width: 128rpx;
    height: 128rpx;
  }
  
  .couponOne .left .money .couponTip {
    font-size: 36rpx;
    display: flex;
    flex-direction: row;
    align-items: baseline;
  }
  
  .couponOne .left .money .couponTip text {
    display: inline-block;
    font-size: 78rpx;
    line-height: 128rpx;
  }
  
  .couponOne .left .info {
    flex: 1;
    font-size: 26rpx;
  }
  
  .couponOne .left .info .name {
    font-size: 30rpx;
    line-height: 1;
    font-weight: 500;
  }
  
  .couponOne .left .info .useCondition {
    line-height: 1;
    font-size: 26rpx;
    margin-top: 16rpx;
  }
  
  .couponOne .left .info .time {
    line-height: 1;
    font-size: 26rpx;
    margin-top: 12rpx;
  }
  
  .coupons .couponOne .right {
    width: 231rpx;
    flex: 1;
    text-align: center;
    padding-top: 43rpx;
  }
  
  .couponOne .right image {
    width: 162rpx;
    height: 44rpx;
    margin: 0 auto;
  }
  
  .couponOne .right view {
    font-size: 30rpx;
    line-height: 53rpx;
  }
  
  /*多个  */
  
  .coupons .couponMore {
    padding-left: 20rpx;
  }
  
  .couponMore .couponList {
    display: flex;
    flex-direction: row;
  }
  
  .couponMore .couponList .couponItem {
    display: inline-flex;
    width: 286rpx;
    height: 156rpx;
    background:#fff url('https://pic.repaiapp.com/static/png/20180130/18/1517307729497349852.png') top right no-repeat;
    background-size: 56rpx 156rpx;
    color: #cecece;
    font-size: 24rpx;
    margin-right: 20rpx;
    box-sizing: border-box;
    border-radius: 6rpx;
    border:1px solid #cecece;
  }
  
  .couponMore .couponList .couponItem .box {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 226rpx; 
    height: 156rpx;
    /* padding-right: 60rpx; */
  }
  
  .couponMore .couponList .couponItem .box-right{
    display: inline-block;
    margin: 0 auto;
    text-align: center;
    /* justify-content: center; */
    line-height: 60rpx;
    width: 60rpx;
    font-size: 26rpx;
    color: #fff;
    writing-mode: vertical-lr;
    letter-spacing: 2rpx;
  }
  
  .couponMore .couponItem .couponMoney {
    display: flex;
    justify-content: center;
     align-items: baseline; 
  }
  
  .couponMore .couponItem .couponMoney text {
    font-size: 26rpx;
  }
  
  .couponMore .couponItem .couponMoney view {
    font-size: 52rpx;
    line-height: 55rpx;
    padding-bottom: 3rpx;
  }
  
  .couponMore .couponItem .condition {
    font-size: 24rpx;
    max-width: 200rpx;
    padding: 0 20rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    color: #fff;
    background: #cecece;
    border-radius: 18rpx;
    margin-top: 10rpx;
  }
  
  .couponMore .couponItem.active {
    background: #fff url('https://pic.repaiapp.com/static/png/20180130/19/1517310612406902102.png') top right no-repeat;
    color: #c2a46b;
    border-color: #c2a46b;
    background-size: 56rpx 156rpx;
  }
  
  .couponMore .couponItem.over {
      background: #fff url('https://pic.repaiapp.com/pic/e2/66/16/e266165d5c65d674c0d90003e1afe686966fc946.png') top right no-repeat;
      background-size: 56rpx 156rpx;
  }
  
  
  
  .couponMore .couponItem.active .condition {
    color: #fff;
    background: #c2a46b;
  }
  
  /*导航部分  */
  
