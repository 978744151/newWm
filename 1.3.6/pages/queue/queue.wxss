.blur {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    filter: blur(10px);
}

.queue-logo {
    width: 138rpx;
    height: 138rpx;
    margin-right: 35rpx;
}

.queue-logo image {
    width: 138rpx;
    height: 138rpx;
}

.qu-news .shoptit {
    font-size: 30rpx;
    color: #ffffff;
}

.qu-news .shoptype {
    font-size: 25rpx;
    color: #ffffff;
    margin-top: 15rpx;
}

.qu-news .shoptype text {
    margin-right: 25rpx;
}

.shoptable {
    background: #fff;
    margin-bottom: 25rpx;
}

.tablenav {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    height: 72rpx;
    align-items: center;
    padding: 0 35rpx;
}

.tablenav text {
    display: block;
    font-size: 28rpx;
    color: #333333;
}

.table-type {
    width: 204rpx;
}

.table-num {
    width: 177rpx;
    text-align: right;
}

.table-time {
    width: 261rpx;
    text-align: right;
}

.tablediv {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    height: 106rpx;
    align-items: center;
    border-bottom: 1rpx solid #f2f2f2;
    padding: 0 35rpx;
}

.table-type .type-p1 {
    font-size: 30rpx;
    color: #333333;
}

.table-type .type-p2 {
    display: block;
    font-size: 25rpx;
    color: #7d7d7d;
    margin-top: 5rpx;
}

.tablediv .type-p3 {
    font-size: 30rpx;
    color: #333333;
}

.tablediv .type-p3 text {
    color: #fc4e4e;
    font-size: 47rpx;
}

.nowtable {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    height: 120rpx;
    align-items: center;
    justify-content: space-between;
}

.nowtable-left {
    margin-left: 35rpx;
}

.nowtable-p1,
.nowtable-p2 {
    font-size: 28rpx;
    color: #7d7d7d;
}

.nowtable-p2 {
    margin-top: 5rpx;
}

.nowtable-p1 text,
.nowtable-p2 text {
    color: #333333;
}

.tctable {
    width: 276rpx;
    height: 91rpx;
    background: #fc4e4e;
    line-height: 91rpx;
    text-align: center;
    border-radius: 5rpx;
    font-size: 28rpx;
    color: #fff;
    margin-right: 35rpx;
}

.fartable {
    width: 276rpx;
    height: 91rpx;
    background: #bbbbbb;
    line-height: 91rpx;
    text-align: center;
    border-radius: 5rpx;
    font-size: 28rpx;
    color: #fff;
    margin-right: 45rpx;
}

.table-hint {
    background: #fff;
    box-sizing: border-box;
    padding: 25rpx 0 25rpx 30rpx;
    border-top: 1rpx solid #f2f2f2;
}

.table-hint text {
    display: block;
    font-size: 26rpx;
    color: #7d7d7d;
    line-height: 40rpx;
}

.mask {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.55;
    z-index: 11;
}

.peoplediv {
    position: fixed;
    left: 50%;
    top: 50%;
    width: 632rpx;
    height: 740rpx;
    margin-left: -316rpx;
    margin-top: -344rpx;
    background: #fff;
    border-radius: 18rpx;
    z-index: 22;
}

.people-hint {
    font-size: 32rpx;
    color: #000000;
    margin-top: 40rpx;
    text-align: center;
}

.bus-hint {
    font-size: 28rpx;
    color: #7d7d7d;
    margin: 30rpx 0 0 20rpx;
}

.people-number {
    display: block;
    font-size: 28rpx;
    margin: 20rpx 0 28rpx;
    text-align: center;
    color: #fc4e4e;
}

.numlist {
    height: 344rpx;
    overflow: hidden;
}

.number,
.numbermore {
    float: left;
    width: 119rpx;
    height: 90rpx;
    margin: 20rpx 0 0 28rpx;
    border: 1rpx solid #d4d4d4;
    border-radius: 4rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 32rpx;
    color: #424242;
}

.peoplebutton {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    justify-content: space-around;
    margin-top: 40rpx;
}

.peoplefalse {
    width: 258rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    background: #e9e9e9;
    border: 1rpx solid #d4d4d4;
    border-radius: 4rpx;
    font-size: 28rpx;
}

.peopletrue {
    width: 258rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    border: 1rpx solid #d4d4d4;
    border-radius: 4rpx;
    font-size: 28rpx;
}

.choose-hint {
    height: 72rpx;
    overflow: hidden;
}

.business {
    background: #fff;
    margin-bottom: 25rpx;
}

.business-list {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    padding: 0 25rpx 0 20rpx;
    align-items: center;
    height: 105rpx;
    border-bottom: 1px solid #f0f0f0;
}

.business-list:last-child {
    border-bottom: none;
}

.business-list .phone-icon {
    width: 30rpx;
    height: 31rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 28rpx 100%;
}

.area-icon {
    width: 30rpx;
    height: 34rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 26rpx 100%;
}

.time-icon {
    width: 30rpx;
    height: 29rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.business-list .bus-text {
    font-size: 28rpx;
    color: #7d7d7d;
    margin-left: 20rpx;
    flex: 1;
}

.scene {
    background: #fff;
    margin-bottom: 30rpx;
}

.scene-tit {
    height: 105rpx;
    line-height: 105rpx;
    padding-left: 25rpx;
    font-size: 30rpx;
    color: #333333;
    border-bottom: 1px solid #f0f0f0;
}

.scene-img {
    padding-bottom: 20rpx;
}

.scene-img image {
    width: 220rpx;
    height: 220rpx;
    margin: 22rpx 0 0 22rpx;
}

.bus-detail {
    width: 13rpx;
    height: 26rpx;
    background: url(data:image/png;base64,);
    background-size: 100% 100%;
    text-align: right;
}

.shop-say {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
    align-items: center;
    margin: 18rpx 0;
}

.shop-price {
    font-size: 22rpx;
    color: #ffffff;
    margin-left: 15rpx;
}

.shop-xx {
    display: flex;
    display: -webkit-flex;
    flex-direction: row;
}

.xx-hua {
    width: 23rpx;
    height: 22rpx;
    background: url(https://pic.repaiapp.com/pic/00/7f/ad/007fadce23f4d13d7642754611abbfdefb1cbd6c.png);
    background-size: 100% 100%;
    margin-right: 5rpx;
}

.xx-hui {
    width: 23rpx;
    height: 22rpx;
    background: url(https://pic.repaiapp.com/pic/ad/4f/79/ad4f79f216fbc5c9fc05f7abb9c39097939e39b6.png);
    background-size: 100% 100%;
    margin-right: 5rpx;
}
