    position: relative;
    display: flex;
    background: url(https://pic.repaiapp.com/pic/7f/61/6b/7f616bd2481ebaf734582c44e0c1ad3da34f0a63.png);
    align-items: center;
    height: 195rpx;
    padding: 0 24rpx;
    /*filter: blur(10px);*/
}


