.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 100rpx;
    /* border-top: 1rpx solid gray;  */
}

.tabbar_nav{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    height: 100%;
    background: #fff;
}
.tabbar_nav::focus{
  background: #fff;
}
.tabbar_icon{
    width: 41rpx;
    height: 41rpx;
    margin-bottom: 3px;
    /*margin-top: 1px;*/
}
.tabword{
    font-size: 24rpx;

}